{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "react-hooks",
    "jest"
  ],
  "rules": {
    "no-console": 1,
    "require-atomic-updates": 1,
    "eqeqeq": 2,
    "react/prop-types": 0,
    "no-unused-vars": 0,
    "object-curly-spacing": 0,
    "quotes": 0,
    "require-jsdoc": 0,
    "max-len": 0,
    "eol-last": 0,
    "indent": 0,
    "new-cap": 0,
    "valid-jsdoc": 0,
    "arrow-parens": 0,
    "padded-blocks": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}