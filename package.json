{
  "name": "studyfind-researcher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f ./.env.production react-scripts start",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css,html,json}'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest test --passWithNoTests",
    "build": "react-scripts build",
    "stage": "env-cmd -f ./.env.development firebase deploy --only hosting --project studyfind-development",
    "deploy": "env-cmd -f ./.env.production firebase deploy --only hosting --project studyfind-researcher"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "firebase": "^8.6.3",
    "flesch": "^2.0.0",
    "framer-motion": "^4",
    "moment-timezone": "^0.5.33",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^4.0.3",
    "react-sortable-hoc": "^2.0.0",
    "react-use": "^17.2.4",
    "styled-components": "^5.3.0",
    "syllable": "^5.0.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/sortablejs": "^1.10.6",
    "env-cmd": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "firebase-tools": "^8.13.1",
    "jest-styled-components": "^7.0.3"
  },
  "_moduleAliases": {
    "components": "./src/components",
    "context": "./src/context",
    "database": "./src/database",
    "hooks": "./src/hooks",
    "images": "./src/images",
    "pages": "./src/pages",
    "templates": "./src/templates",
    "utils": "./src/utils"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
